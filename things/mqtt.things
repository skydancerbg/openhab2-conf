        Type number : totalyest "Total energy yesterday" [ stateTopic="tele/sonoff_pow_1/SENSOR", transformationPattern="JSONPATH:$.ENERGY.Yesterday"]
        Type number : rssi      "WiFi Signal Strength"   [ stateTopic="tele/sonoff_pow_1/STATE", transformationPattern="JSONPATH:$.Wifi.RSSI"]
    } 


