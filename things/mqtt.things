// // ESP8266(TASMOTA) KITCHEN REFRIGERATOR WATER VALVE
    Thing topic fridge_water_valve "Refrigerator water valve" @ "Kitchen" {
    Channels:
        Type string : reachable     "Reachable"            [ stateTopic="tele/kitchen_water_valve/LWT" ]
        Type switch : openvalve     "Open water valve"    [ stateTopic="stat/kitchen_water_valve/POWER1", commandTopic="cmnd/kitchen_water_valve/POWER1" ]
        Type switch : closevalve    "Close water valve"   [ stateTopic="stat/kitchen_water_valve/POWER2", commandTopic="cmnd/kitchen_water_valve/POWER2" ]     
        Type switch : isfullyopen     "Open water valve"    [ stateTopic="stat/kitchen_water_shutoff_feedback/POWER3", commandTopic="cmnd/kitchen_water_shutoff_feedback/POWER3" ]
        Type switch : isfullyclosed    "Close water valve"   [ stateTopic="stat/kitchen_water_shutoff_feedback/POWER4", commandTopic="cmnd/kitchen_water_shutoff_feedback/POWER4" ]     
        Type string : valveopened "valve opened" [ stateTopic="tele/kitchen_water_valve/SENSOR", transformationPattern="JSONPATH:$.Switch3"]
        Type string : valveclosed "valve closed" [ stateTopic="tele/kitchen_water_valve/SENSOR", transformationPattern="JSONPATH:$.Switch4"]
        Type number : rssi          "WiFi Signal Strength" [ stateTopic="tele/kitchen_water_valve/STATE", transformationPattern="JSONPATH:$.Wifi.RSSI"]
    }

