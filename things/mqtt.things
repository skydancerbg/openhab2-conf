// // SONOFF POW
    
    // // RECUPERATOR on sonoff_pow_1 
    // https://community.openhab.org/t/solved-sonoff-pow-openhab2-integration/77122/8 
    
    Thing topic recuperator "Recuperator" @ "Living Room" {
    Channels:
        Type string : reachable "Reachable"              [ stateTopic="tele/sonoff_pow_1/LWT" ]
        Type switch : power     "Power"                  [ stateTopic="stat/sonoff_pow_1/POWER", commandTopic="cmnd/sonoff_pow_1/POWER" ]
