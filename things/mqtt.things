
// Thing <binding_id>:<type_id>:<thing_id> "Label" @ "Location" [ <parameters> ]

// Bridge mqtt:broker:mosquitto "Mosquitto" [ host="localhost", port=1883, secure=false, username="openhabian", password="Cessna152", clientID="openHAB2" ]
Bridge mqtt:broker:MyMQTTthing "MyMQTTthing" [ host="127.0.0.1", port=1883, secure=false, username="openhabian", password="Cessna152", clientID="oh2" ]

{
//    // MQTT DIY MAILBOX SENSOR
    Thing topic mailbox_sensor "Mailbox sensor" @ "Outside stairs" {
    Channels:
        Type string : reachable "Reachable"              [ stateTopic="tele/WiFi_Mailbox_sensor/LWT" ]
        Type switch : resetmailstate     "Reset gotMail state"                [ stateTopic="stat/WiFi_Mailbox_sensor/State", commandTopic="cmnd/WiFi_Mailbox_sensor/ResetMailState" ]
        Type switch : restartcontroler     "Restart controler"                [ commandTopic="cmnd/WiFi_Mailbox_sensor/RestartController" ]        
        // Type switch : restartcontroler     "Restart controler"                [ stateTopic="stat/WiFi_Mailbox_sensor/State", commandTopic="cmnd/WiFi_Mailbox_sensor/RestartController" ]        
        // Type number : rssi      "WiFi Signal Strength" [ stateTopic="tele/sonoff_dual_1/STATE", transformationPattern="JSONPATH:$.Wifi.RSSI"]
        Type string : havemail      "Got mail?" [ stateTopic="stat/WiFi_Mailbox_sensor/HaveMail"]
        Type string : mailboxdoor      "Mailbox door" [ stateTopic="stat/WiFi_Mailbox_sensor/MailboxDoor"]
    }


//    // SONOF DEVICES

 // // SONOFF DUAL   
    Thing topic sonoff_dual "Sonoff Dual" @ "Living Room" {
    Channels:
        Type string : reachable "Reachable"              [ stateTopic="tele/sonoff_dual_1/LWT" ]
        Type switch : power1     "Power1"                [ stateTopic="stat/sonoff_dual_1/POWER1", commandTopic="cmnd/sonoff_dual_1/POWER1" ]
        Type switch : power2     "Power2"                [ stateTopic="stat/sonoff_dual_1/POWER2", commandTopic="cmnd/sonoff_dual_1/POWER2" ]        
        Type number : rssi      "WiFi Signal Strength" [ stateTopic="tele/sonoff_dual_1/STATE", transformationPattern="JSONPATH:$.Wifi.RSSI"]
    }

 // // SONOFF TH16

 // // SONOFF TH16 2 - with DB8 sensor

    Thing topic sonoff_th16_1 "SonoffTH16 1 DS18B20 - T" @ "Living Room" {
    Channels:
        Type string : reachable     "Reachable"             [ stateTopic="tele/sonoff_th16_1/LWT" ]
        Type switch : power         "Power"                 [ stateTopic="stat/sonoff_th16_1/POWER", commandTopic="cmnd/sonoff_th16_1/POWER" ]
        Type number : temperature   "Temperature"           [ stateTopic="tele/sonoff_th16_1/SENSOR", transformationPattern="JSONPATH:$.DS18B20.Temperature" ]
        Type number : rssi      "WiFi Signal Strength"      [ stateTopic="tele/sonoff_th16_1/STATE", transformationPattern="JSONPATH:$.Wifi.RSSI"]
    }

    Thing topic sonoff_th16_2 "SonoffTH16 2 AM2301 T/H" @ "Living Room" {
    Channels:
        Type string : reachable     "Reachable"             [ stateTopic="tele/sonoff_th16_2/LWT" ]
        Type switch : power         "Power"                 [ stateTopic="stat/sonoff_th16_2/POWER", commandTopic="cmnd/sonoff_th16_2/POWER" ]
        Type number : temperature   "Temperature"           [ stateTopic="tele/sonoff_th16_2/SENSOR", transformationPattern="JSONPATH:$.AM2301.Temperature" ]
        Type number : humidity      "Humidity"              [ stateTopic="tele/sonoff_th16_2/SENSOR", transformationPattern="JSONPATH:$.AM2301.Humidity" ]        
        Type number : rssi      "WiFi Signal Strength"      [ stateTopic="tele/sonoff_th16_2/STATE", transformationPattern="JSONPATH:$.Wifi.RSSI"]
    }

